{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-svelte"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "tailwindStylesheet": "./src/routes/layout.css",
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "bracketSameLine": true
}
